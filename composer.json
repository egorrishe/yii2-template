{
    "name": "egorrishe/test-yii2",
    "description": "project for test purposes",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "email": "sankovegorrishe@gmail.com",
        "source": "https://github.com/egorrishe/test-yii2-blog"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.2.*",
        "ext-PDO": "*",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "2amigos/yii2-ckeditor-widget": "^2.2",
        "kop/yii2-scroll-pager": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-phpbrowser": "^1.0",
        "heroku/heroku-buildpack-php": "^172.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
